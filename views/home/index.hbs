<div class="container body-content">
    <div class="row">
        {{#each categories}}
            <div class="col-md-6">
                    <header>
                        <h2>
                            <br><a class="btn btn-default" style="background-color: #2c3e50" href="/category/{{this.id}}"><img src="{{this.imagePath}}" width="400" height="150" alt="{{this.name}}"></a>
                        </h2>
                    </header>
            </div>
            {{/each}}
    </div>
</div>